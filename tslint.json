{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "semicolon": [true, "never"],
        "no-console": false,
        "quotemark": [true, "single", "avoid-escape", "avoid-template"],
        "trailing-comma": [true, {"multiline": "never", "singleline": "never"}],
        "member-ordering": false,
        "object-literal-sort-keys": false
    },
    "rulesDirectory": []
}